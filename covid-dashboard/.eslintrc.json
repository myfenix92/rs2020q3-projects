{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "indent": ["error", 4],
        "import/no-cycle": "off",
        "max-len":"off",
        "no-param-reassign":"off",
        "no-use-before-define":"off",
        "no-shadow":"off",
        // "no-unused-vars":"off",
        "prefer-const":"off"
    }
}
