{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },

  //"parser": "babel-eslint",

  "extends": ["airbnb/base"],
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
  ],
    "semi": "error",
    "quotes": "off",
    "no-use-before-define": "off",
    "no-param-reassign": "off"
  }
}